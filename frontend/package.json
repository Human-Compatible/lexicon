{
  "scripts": {
    "start": "expo start",
    "quickstart": "expo -c",
    "typecheck": "tsc --noEmit -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "jest": "jest --coverage --passWithNoTests",
    "test": "yarn graphql:generate && yarn lint && yarn typecheck && yarn format:check && yarn jest",
    "eject": "expo eject",
    "graphql:generate": "graphql-codegen --config codegen.ts",
    "postinstall": "patch-package",
    "eas-build-post-install": "yarn --cwd ../ && yarn --cwd ../ generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@expo-google-fonts/courier-prime": "^0.2.2",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/elements": "^1.3.4",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "apollo-upload-client": "^17.0.0",
    "expo": "^46.0.0",
    "expo-constants": "~13.2.4",
    "expo-crypto": "~11.0.0",
    "expo-file-system": "~14.1.0",
    "expo-image-picker": "~13.3.1",
    "expo-linking": "3.2.4",
    "expo-localization": "~13.1.0",
    "expo-network": "~4.3.0",
    "expo-permissions": "~13.2.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-updates": "~0.14.7",
    "graphql": "^16.5.0",
    "intl": "^1.2.5",
    "markdown-it-flowdock": "^0.3.8",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^6",
    "react-native": "0.69.6",
    "react-native-auto-grow-textinput": "^1.2.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-keyboard-accessory": "^0.1.16",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-parsed-text": "^0.0.22",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-toast-message": "^2.1.5",
    "use-debounce": "^8.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.6",
    "@graphql-codegen/near-operation-file-preset": "^2.4.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/jest": "^26.0.9",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.6.0",
    "eslint-config-kodefox": "^1.2.0",
    "eslint-plugin-styles": "^0.1.0",
    "eslint-plugin-t": "^1.2.1",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "react-native-console-time-polyfill": "^1.2.3",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "18.0.0",
    "runtypes": "^5.0.1",
    "ts-jest": "^26.1.4",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/src/__mocks__/setup.js"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/src/__mocks__/svgMock.js"
    }
  },
  "eslintConfig": {
    "extends": "kodefox/react-native",
    "plugins": [
      "t",
      "styles"
    ],
    "rules": {
      "t/string-literal": 1,
      "styles/style-maker-no-unused": 1,
      "operator-assignment": [
        "warn",
        "always"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true
}
